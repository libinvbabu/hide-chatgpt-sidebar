{
    "manifest_version": 3,
    "name": "ChatGPT Sidebar Toggle",
    "version": "1.0",
    "description": "Toggle the sidebar of chatGPT website",
    "host_permissions": ["activeTab"],
    "background": {
    "service_worker": "background.js"
    },
    "action": {
    "default_icon": {
        "16": "icon-16.png",
        "32": "icon-32.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "default_title": "Toggle Sidebar",
    "default_popup": "popup.html"
    }
}